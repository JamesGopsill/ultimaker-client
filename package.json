{
  "name": "@jamesgopsill/ultimaker-client",
  "version": "0.8.1",
  "description": "Client for interacting with the Ultimaker API",
  "keywords": [
    "ultimaker",
    "3d printing",
    "additive manufacturing"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "James Gopsill",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesgopsill/ultimaker-client"
  },
  "bugs": {
    "url": "https://github.com/jamesgopsill/ultimaker-client/issues"
  },
  "homepage": "https://jamesgopsill.github.io/ultimaker-client/",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test:airmanager": "jest airmanager.test.ts",
    "test:materials": "jest materials.test.ts",
    "test:history": "jest history.test.ts",
    "test:get-methods": "jest get-methods.test.ts",
    "test:all": "jest",
    "build": "tsc",
    "docs": "typedoc",
    "format": "prettier --write src/**/* && prettier --write tests/**/*",
    "pre-add": "yarn format && yarn docs && node utils/clean-dist.js && yarn tsc && node utils/clean-crlf.js"
  },
  "dependencies": {
    "cross-fetch": "^3.1.4",
    "form-data": "^4.0.0",
    "is-ip": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.9",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.9",
    "typescript": "^4.4.4"
  }
}
