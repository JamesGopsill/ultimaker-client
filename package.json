{
  "name": "ultimaker-client",
  "version": "0.1.0",
  "description": "Client for interacting with the Ultimaker API",
  "keywords": [
    "ultimaker",
    "3d printing",
    "additive manufacturing"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "James Gopsill",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesGopsill/ultimaker-client"
  },
  "bugs": {
    "url": "https://github.com/JamesGopsill/ultimaker-client/issues"
  },
  "homepage": "https://jamesgopsill.github.io/ultimaker-client/",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test:no-printer": "jest tests/instantiation.test.ts",
    "test:all": "jest --runInBand",
    "test:job": "jest --runInBand tests/job.test.ts",
    "test:system": "jest --runInBand tests/system.test.ts",
    "build": "tsc",
    "docs": "typedoc",
    "format": "prettier --write src/**/* && prettier --write tests/**/*",
    "pre-add": "yarn format && yarn docs && yarn tsc"
  },
  "dependencies": {
    "cross-fetch": "^3.1.4",
    "form-data": "^4.0.0",
    "is-ip": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.9",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.9",
    "typescript": "^4.4.4"
  }
}
